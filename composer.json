{
    "name": "enzaime/dynamic-link",
    "description": "Enzaime Firebase Dynamic Link",
    "license": "MIT",
    "authors": [
        {
            "name": "Mahbub Rabbani",
            "email": "mahbub.rucse@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Enzaime\\DynamicLink\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Enzaime\\DynamicLink\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Enzaime\\DynamicLink\\ServiceProvider"
            ],
            "aliases": {
                "EnzDynamicLink": "Enzaime\\DynamicLink\\Facades\\EnzDynamicLink"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0",
        "laravel/pint": "^1.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter",
        "test-w": "vendor\\bin\\phpunit",
        "test-wf": "vendor\\bin\\phpunit --filter"
    }
}
